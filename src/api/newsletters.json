title: API - Newsletter
permalink: api/newsletters
---
[
{% for newsletter in site.newsletters %}
  {
    "url": {{ newsletter['url'] | jsonify }},
    "title": {{ newsletter['title'] | jsonify }},
    "slug": {{ newsletter['slug'] | jsonify }},
    "item_date": {{ newsletter['item_date'] | jsonify }},
    "date": {{ newsletter['date'] | jsonify }},
    "categories": {{ newsletter['categories'] | jsonify }},
    "featured_image": {{ newsletter['featured_image'] | jsonify }},
    "summary": {{ newsletter['summary'] | jsonify }},
    "link": {{ newsletter['link'] | jsonify }},
    "content": {{ article['content'] | jsonify }}
  }{% unless forloop.last %},{% endunless %}
{% endfor %}
]