---
layout: none
title: API - Press
permalink: api/press
---
[
  {% assign sorted = site.press | sort: 'item_date'  %}
  {% for press in sorted reversed %}
  {% assign id = 'press-' | append: forloop.index %}
  {
    "id": {{ id | jsonify }},
    "url": {{ press['url'] | jsonify }},
    "title": {{ press['title'] | jsonify }},
    "slug": {{ press['slug'] | jsonify }},
    "item_date": {{ press['item_date'] | jsonify }},
    "date": {{ press['date'] | jsonify }},
    "categories": {{ press['categories'] | jsonify }},
    "source": {{ press['source'] | jsonify }},
    "featured_image": {{ press['featured_image'] | jsonify }},
    "summary": {{ press['description_html'] | jsonify }},
    "link": {{ press['link'] | jsonify }}
  }{% unless forloop.last %},{% endunless %}
{% endfor %}
]